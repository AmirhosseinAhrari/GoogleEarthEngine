var cor = [49.64500915431612,37.14344151987502]

var geometry = ee.Geometry.Point(cor)

var roi = ee.FeatureCollection("FAO/GAUL/2015/level1")
.filterBounds(geometry).geometry().simplify(1000)

Map.centerObject(roi)
Map.addLayer(roi)

var annual = ee.ImageCollection("projects/sat-io/open-datasets/GLC-FCS30D/annual")
.filterBounds(roi)
.mosaic()

print(annual)

Map.addLayer(annual.clip(roi),[],'landcover',false)

Export.image.toDrive({
  image: annual.clip(roi), 
  description: 'annual_map_guilan', 
  scale: 100, 
  region: roi, 
  crs: 'EPSG:4326', 
  folder: 'test',
  maxPixels: 1e13
  
  })
