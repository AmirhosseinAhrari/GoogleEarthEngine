!pip install --upgrade xee
!pip install -U geemap

import ee

ee.Authenticate()
ee.Initialize(
    project = 'ee-amirhosseinahrari',
    opt_url = 'https://earthengine-highvolume.googleapis.com'
)

import geemap

map = geemap.Map()
map

point= map.draw_last_feature.geometry()
point

border = (
    ee.FeatureCollection("USDOS/LSIB_SIMPLE/2017")
    .filterBounds(point)
    .geometry().simplify(1000)
)

map.addLayer(border)

gilan = (
    ee.FeatureCollection('users/amirhosseinahrari/Gilan')
)

map2 = geemap.Map()
map2

map2.addLayer(gilan,{},'gilan')
